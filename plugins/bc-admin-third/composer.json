{
    "name": "baserproject/bc-admin-third",
    "description": "theme for baserCMS",
    "homepage": "https://basercms.net",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0",
        "baserproject/baser-core": "5.0.0-alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "BcAdminThird\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BcAdminThird\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
